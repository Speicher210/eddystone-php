{
  "name": "wingu/eddystone",
  "license": "proprietary",
  "description": "wingu analytics",
  "type": "project",
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Wingu\\Eddystone\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wingu\\Eddystone\\Test\\": "test/"
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
